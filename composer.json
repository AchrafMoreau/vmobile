  "require": {
    "php": ">=7.1.3",
    "doctrine/orm": "^2.9",
    "doctrine/dbal": "^2.13"


  },
  "require-dev": {
    "prestashop/php-dev-tools": "^4.3"
  },
  "config": {
    "preferred-install": "dist",
    "classmap-authoritative": true,
    "optimize-autoloader": true,
